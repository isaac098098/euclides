alt + s
    rofi -show select -modes "select:$HOME/.config/rofi/scripts/select.sh"

alt + n
    alacritty -e nvim $HOME/notes/current-notes/main.tex

ctrl + shift + n
    zathura $HOME/notes/current-notes/main.pdf

alt + p
    rofi -show pdf -modes "pdf:$HOME/.config/rofi/scripts/pdf.sh"

alt + o
    rofi -markup-rows -show open -modes "open:$HOME/.config/rofi/scripts/open.sh"

ctrl + shift + a
    rofi -show "add inkscape" -modes "add inkscape:$HOME/euclides/bin/rofi_inkscape_add.sh"

ctrl + shift + e
    rofi -show "edit inkscape" -modes "edit inkscape:$HOME/euclides/bin/rofi_inkscape_edit.sh"

ctrl + shift + q
    bash $HOME/euclides/bin/inkscape_save.sh

ctrl + shift + p
    bash $HOME/euclides/bin/edtex.sh
