ctrl + shift + b
    rofi -show books -modes "books:$HOME/.config/rofi/scripts/search_books.sh"

ctrl + shift + d
    rofi -show select_books -modes "select:$HOME/.config/rofi/scripts/select_books.sh"

ctrl + shift + o
    rofi -markup-rows -show edit_notes -modes "edit:$HOME/.config/rofi/scripts/edit_notes.sh"

ctrl + shift + s
    rofi -show select -modes "select:$HOME/.config/rofi/scripts/select_course.sh"

ctrl + shift + p
    rofi -show compile_notes -modes "compile:$HOME/.config/rofi/scripts/compile_notes.sh"

ctrl + shift + i
    rofi -show select_zettelkasten -modes "select:$HOME/.config/rofi/scripts/select_zettelkasten.sh"

ctrl + shift + n
    rofi -show create_card -modes "create:$HOME/.config/rofi/scripts/create_card.sh"

ctrl + shift + e
    rofi -show edit_card -modes "edit:$HOME/.config/rofi/scripts/edit_card.sh"

ctrl + shift + q
    rofi -show delete_card -modes "delete:$HOME/.config/rofi/scripts/delete_card.sh"

ctrl + shift + y
    zathura $HOME/zettelkasten/current/main.pdf

ctrl + shift + u
    zathura $HOME/notes/current-notes/main.pdf

ctrl + shift + h
    rofi -show warmup -modes "select:$HOME/.config/rofi/scripts/warmup.sh"

alt + y
    alacritty --working-directory $HOME/todo/
