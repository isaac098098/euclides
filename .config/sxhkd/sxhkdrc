ctrl + shift + b
    rofi -show search_book -config "$HOME/.config/rofi/short.rasi" -modes "search_book:$HOME/.config/rofi/scripts/bash/search_book.sh"

ctrl + shift + d
    rofi -show select_books -config "$HOME/.config/rofi/short.rasi" -modes "select_books:$HOME/.config/rofi/scripts/bash/select_books.sh"

ctrl + shift + e
    rofi -show edit_card -config "$HOME/.config/rofi/long.rasi" -modes "edit_card:$HOME/.config/rofi/scripts/bash/edit_card.sh"

ctrl + shift + o
    rofi -markup-rows -show edit_notes -config "$HOME/.config/rofi/long.rasi" -modes "edit_notes:$HOME/.config/rofi/scripts/bash/edit_notes.sh"

ctrl + shift + s
    rofi -show select_notes -config "$HOME/.config/rofi/short.rasi" -modes "select_notes:$HOME/.config/rofi/scripts/bash/select_notes.sh"

ctrl + shift + p
    rofi -show compile_notes -config "$HOME/.config/rofi/long.rasi" -modes "compile_notes:$HOME/.config/rofi/scripts/bash/compile_notes.sh"

ctrl + shift + u
    zathura $HOME/notes/current-notes/main.pdf

ctrl + shift + h
    rofi -show warmup -modes "select:$HOME/.config/rofi/scripts/bash/warmup.sh"

alt+y
    alacritty --working-directory $HOME/todo
